# Environment variables for Archimedes POC1

# Database Configuration
DATABASE_URL=postgresql://archimedes:password@localhost:5432/archimedes_poc1

# API Keys (get your own from the respective services)
FRED_API_KEY=your_fred_api_key_here  # Get from https://fred.stlouisfed.org/docs/api/api_key.html

# Application Configuration
APP_NAME=Archimedes POC1
DEBUG=True
LOG_LEVEL=INFO

# CORS Configuration (for frontend)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Data Ingestion Schedule
INGESTION_SCHEDULE=0 6 * * *  # Daily at 6 AM UTC (cron format)
